# PromptToProduct Configuration
# Copy this file to .env and fill in your actual values

# User Configuration  
DEFAULT_ASSIGNEE=your_github_username
DEFAULT_OWNER=your_github_username
SYSTEM_USER=PromptToProduct-Agent
DEFAULT_PRIORITY=Medium
DEFAULT_STATUS=In Progress

# GitHub Configuration
GITHUB_PERSONAL_ACCESS_TOKEN=your_github_token_here
GITHUB_REPO_OWNER=your_github_username
GITHUB_REPO_NAME=PromptToProduct
GITHUB_BASE_URL=https://api.github.com

# Banking Domain Configuration
DEFAULT_BANKING_CONTEXT=true
DEFAULT_COMPLIANCE_AREAS=KYC,AML,PCI-DSS
BANKING_PRODUCTS=loans,credit_cards,payments,investments,accounts,digital_banking

# Agent Configuration
ORCHESTRATOR_MEMORY_SIZE=10
SPEC_AGENT_AUTO_VALIDATE=true
CODE_AGENT_DEFAULT_LANGUAGE=python
VALIDATION_AGENT_MIN_SCORE=0.7

# File Paths
SPECS_ROOT_PATH=specs
MYBANK_ROOT_PATH=src/MyBank
PROMPTS_LIBRARY_PATH=prompts

# System Settings
LOG_LEVEL=INFO
DEBUG_MODE=false
AUTO_SYNC_GITHUB=false
SESSION_TIMEOUT=3600

# Validation Settings
VALIDATION_STRICT_MODE=false
SCHEMA_VALIDATION_ENABLED=true
AUTO_CREATE_GITHUB_ISSUES=true

# Development Settings
DEVELOPMENT_MODE=true
TEST_MODE=false
MOCK_GITHUB_API=false